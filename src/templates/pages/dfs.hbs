	<section class="content-section content-section-2">

		<h3>Dine oplysninger</h3>

		<label for="first-name" class="field-wrapper field-wrapper-text">
			<span class="label">Fornavn</span>
			<input id="first-name" name="firstname" type="text" class="input-text" required>
		</label>
		<label for="last-name" class="field-wrapper field-wrapper-text">
			<span class="label">Efternavn</span>
			<input id="last-name" name="lastname" type="text" class="input-text" required>
		</label>
		<label for="email" class="field-wrapper field-wrapper-text">
			<span class="label">E-mail</span>
			<input id="email" name="email_address_" type="text" class="input-text" required>
		</label>
		<label for="phone" class="field-wrapper field-wrapper-text">
			<span class="label">Telefonnr.</span>
			<input id="phone" name="mobile_number_" type="text" class="input-text" required>
		</label>

		<h3>Din adresse</h3>

		<label for="address" class="field-wrapper field-wrapper-text">
			<span class="label">Adresse</span>
			<input id="address" name="postal_street_1_" type="text" class="input-text" required>
		</label>
		<label for="postal-code" class="field-wrapper field-wrapper-text">
			<span class="label">Postnr.</span>
			<input id="postal-code" name="postal_code_" type="text" class="input-text" required>
		</label>
		<label for="city" class="field-wrapper field-wrapper-text">
			<span class="label">By</span>
			<input id="city" name="city_" type="text" class="input-text" required>
		</label>
		<label for="country" class="field-wrapper field-wrapper-select">
			<!--
			<span class="label">Land</span>
			<input id="country" name="country_" type="text" class="input-text" required>
			-->
			<select id="country" name="country_" required>
				<option value="">Land</option>
				<option value="dk">Danmark</option>
				<option value="se">Sverige</option>
				<option value="no">Norge</option>
			</select>
		</label>

		<h3>Børn</h3>
		<label for="children" class="field-wrapper field-wrapper-select">
			<select id="children" name="children" required>
				<option value="">Antal børn</option>
				<option value="0">0</option>
				<option value="1">1</option>
				<option value="2">2</option>
				<option value="3">3</option>
				<option value="4">4</option>
			</select>
		</label>

	{{#times 4}}
		<div class="field-group field-group-child" id="child-{{add this 1}}-group">
			<label for="child-{{add this 1}}-name" class="field-wrapper field-wrapper-text">
				<span class="label">Barnets navn</span>
				<input id="child-{{add this 1}}-name" name="child_{{add this 1}}_name" type="text" class="input-text">
			</label>
